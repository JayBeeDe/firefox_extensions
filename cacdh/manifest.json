{
    "manifest_version": 3,
    "name": "Custom Accentuation Color Dark High Contrast",
    "version": "0.0",
    "description": "Dark High Contrast Theme with customizable accentuation color.",
    "author": "JayBeeDe",
    "browser_specific_settings": {
        "gecko": {
            "id": "@custom-color-dark-contrast"
        }
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "scripting",
        "storage",
        "theme"
    ],
    "options_ui": {
        "page": "options.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "type": "module"
    },
    "icons": {
        "512": "icons/icon-512.png"
    }
}